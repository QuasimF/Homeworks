import

class DemoApplicationTest {

    @Test
    void ItShouldAddNumbers (){
        //given
        int numberOne = 20;
        int numberTwo = 30;

        //when 
        int result = underTest.add(numberOne,numberTwo);

        //then
        int expected = 50
        asserThat(result).isEqualTo(expected);
    }

    class Calculator {
        int add (int a, int b){
            return a+b;
        }
    }
}